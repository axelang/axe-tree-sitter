    "_statement": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "use_stmt"
        },
        {
          "type": "SYMBOL",
          "name": "platform_block"
        },
        {
          "type": "SYMBOL",
          "name": "pub_stmt"
        },
        {
